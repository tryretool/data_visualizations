    "xaxis": {
      "title": "",
      "type": "-",
      "tickformat": "",
      "automargin": true,
      "fixedrange": true,
      "gridcolor": "#fff",
      "zerolinecolor": "#DEDEDE"
    },
    "yaxis": {
      "title": "",
      "type": "linear",
      "tickformat": "",
